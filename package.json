{
  "name": "@nataui/react",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AminNateghi/nataui.git"
  },
  "type": "module",
  "description": "React UI component libraries",
  "license": "MIT",
  "author": "Amin Nateghi",
  "keywords": [
    "React UI component"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "types": "dist/index.d.ts",
  "main": "./dist/nataui.umd.js",
  "module": "./dist/nataui.es.js",
  "exports": {
    ".": {
      "import": "./dist/nataui.es.js",
      "require": "./dist/nataui.umd.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "bugs": {
    "url": "https://github.com/AminNateghi/nataui/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "tsc && vite build -w",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.0.1",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "jsdom": "^21.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.29.4"
  },
  "dependencies": {
    "sass": "^1.60.0"
  }
}